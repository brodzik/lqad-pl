{
    "LAZY_DATASET_READER": 0,
    "SEED": {
        "sampling strategy": "integer",
        "bounds": [0, 100000]
    },
    "GRADIENT": {
        "sampling strategy": "uniform",
        "bounds": [0.5, 1.0]
    },
    "LEARNING_RATE": {
        "sampling strategy": "loguniform",
        "bounds": [1e-6, 1e-1]
    },
    "EPSILON": {
        "sampling strategy": "loguniform",
        "bounds": [1e-10, 1e-6]
    },
    "CUDA_DEVICE": 0,
    "EVALUATE_ON_TEST": 0,
    "BATCH_SIZE": 48,
    "NUM_EPOCHS": 5,
    "MODEL": {
        "sampling strategy": "choice",
        "choices": ["henryk/bert-base-multilingual-cased-finetuned-polish-squad1", "henryk/bert-base-multilingual-cased-finetuned-polish-squad2"]
    }
}
